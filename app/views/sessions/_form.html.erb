<%= form_for(:session, url: sessions_path) do |f|%>

<%= f.label :email %>
<%= f.text_field :email %>

<%= f.label :password %>
<%= f.password_field :password %>

<div class="actions">
<%= f.submit "Sign In", class: "btn"%>
</div>

<% end %>

<p> New User? <%= link_to "Sign up now!", signup_path %> </p>